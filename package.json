{
  "name": "semantic-release-exercise",
  "version": "1.0.0",
  "description": "semantic-release-exercise",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "next",
        "prerelease": "rc"
      }
